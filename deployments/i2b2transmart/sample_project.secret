## nginx
CERT=none
CHAIN=none
KEY=none

## nginx / dockergen
VIRTUAL_HOST=test.com

## i2b2transmart / i2b2-wildfly
# database
DB_HOST=db
DB_USER=BIOMART_USER
DB_PASSWORD=biomart_user
DB_PORT=1521
DB_DB=ORCLPDB1

## i2b2-wildfly
# database user passwords
I2B2HIVE=demouser
I2B2DEMODATA=demouser
I2B2METADATA=demouser
I2B2PM=demouser
I2B2WORKDATA=demouser

## i2b2transmart
# emails
# admin
ADMIN_EMAIL=example@gmail.com

# send emails from this account:
EMAIL_USER=example@gmail.com
EMAIL_PASS=email_pass

# guest login
GUEST_AUTO_LOGIN=false
GUEST_USER=guest

# Auth0
AUTH0_ACTIVE=false
AUTH0_DOMAIN=domain.auth0.com
AUTH0_WEBTASK_URL=https://webtask.io

# Auth0 registration
AUTH0_REGISTRATION=true

# Auth0 admin create
AUTH0_ADMIN_CREATE=false
AUTH0_ADMIN_USER=admin
AUTH0_ADMIN_PASSWORD=admin_pass

# required for JWT tokens between i2b2/tranSMART <-> PIC-SURE
CLIENT_SECRET=dummysecret
CLIENT_ID=dummyid

## pic-sure
# database
IRCT_DB_HOST=irctdb
IRCT_DB_PORT=3306
IRCT_DB_CONNECTION_USER=root
IRCT_DB_PASSWORD=password

## splunk
SPLUNK_STACK_TAG=
SPLUNK_USER=
SPLUNK_FORWARD_SERVER=
SPLUNK_FORWARD_SERVER_ARGS=--accept-license --no-prompt --answer-yes
SPLUNK_DEPLOYMENT_SERVER=
